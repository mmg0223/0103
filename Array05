package array;

import java.util.Arrays;

public class ArrayTest05 {

	public static void main(String[] args) {
		// 배열의 기능을 모아놓은 클래스를 제공합니다.
		int[] arr = {45, 1, 22, 5, 9};
		for(int n : arr) {
			System.out.println(n);
		}
		System.out.println("----");
		
		Arrays.sort(arr); // 정렬
		for(int n : arr) {
			System.out.println(n);
		}

	}

}
